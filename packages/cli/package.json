    "@types/express": "^5.0.2",
    "@types/helmet": "^4.0.0",
    "@types/multer": "^1.4.12",
    "bun": "^1.2.13",
    "chokidar": "^4.0.3",
    "execa": "^7.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.0",
    "octokit": "^4.1.3",
    "path-to-regexp": "^8.2.0",
    "semver": "^7.7.2",
