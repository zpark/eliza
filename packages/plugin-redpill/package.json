{
  "name": "@elizaos/plugin-redpill",
  "version": "1.0.0-beta.10",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos-plugins/plugin-redpill"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ai-sdk/openai": "^1.1.9",
    "@ai-sdk/ui-utils": "1.1.9",
    "@elizaos/core": "^1.0.0-beta.10",
    "ai": "^4.1.25",
    "js-tiktoken": "^1.0.18",
    "tsup": "8.4.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "prettier --write ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "REDPILL_API_KEY": {
        "type": "string",
        "description": "API key for the service"
      }
    }
  },
  "gitHead": "601099e5940ddaee3983d34c529df623d4d64b54",
  "devDependencies": {
    "prettier": "3.5.3"
  }
}
