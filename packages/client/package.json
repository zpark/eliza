  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "start:client": "vite",
    "dev:client": "vite --host 0.0.0.0 --port 5173",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --write ./src",
    "format": "prettier --write ./src",
